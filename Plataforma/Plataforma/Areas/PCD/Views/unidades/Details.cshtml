@model Plataforma.Areas.PCD.Models.unidade

@{
    ViewBag.Title = "Detalles";
}

<div class="col-md-12">
    <h1 class="class-forms">
        <i class="fa fa-user-circle" aria-hidden="true"></i>
        Detalles
    </h1>
    <h6>Información asociada a la unidad</h6>
    <br />
    <h4 class="class-forms">Unidad</h4>
    <dl class="dl-horizontal">
        <dt>
            Unidad
        </dt>
        <dd>
            @Html.DisplayFor(model => model.unidad)
        </dd>
    </dl>
    <div class="col-lg-9">
        <p>
            @Html.ActionLink("Editar", "Edit", new { id = Model.id }) |
            @Html.ActionLink("Volver al indice", "Index")
        </p>
    </div>
</div>
