@model Plataforma.Areas.PCD.Models.tipo_colegio

@{
    ViewBag.Title = "Detalles";
}
<div class="col-md-12">
    <h1 class="class-forms">
        <i class="fa fa-user-circle" aria-hidden="true"></i>
        Detalles
    </h1>
    <h6>Información asociada al tipo de colegio</h6>
    <br />
    <h4 class="class-forms">Tipo de Colegio</h4>
    <dl class="dl-horizontal">
        <dt>
            Tipo de colegio
        </dt>
        <dd>
            @Html.DisplayFor(model => model.tipo_institucion)
        </dd>
    </dl>
    <p>
        @Html.ActionLink("Editar", "Edit", new { id = Model.id }) |
        @Html.ActionLink("Volver al indice", "Index")
    </p>
</div>

