
@{
    ViewBag.Title = "Agregar Datos Tarjeta";
}

<h2 class="class-forms">Agregar tarjeta</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal col-lg-9">
        <h1 class="class-forms"><i class="fa fa-user-circle" aria-hidden="true"></i> &nbsp;Comprador</h1>
        <h6>Agregue los datos solicitados para poder realizar su compra</h6>
        <br><br>
        <div class="form-group">
            <label id="tipoTarjeta" for="tipoTarjeta" class="control-label col-md-4">Tipo de identificación</label>
            <div class="col-md-10">
                <select class="form-control" id="tipoTarjeta" name="tipoTarjeta">
                    <option value="1">Visa</option>
                    <option value="2">MasterCard</option>
                    <option value="3">JCB</option>
                    <option value="4">Discover</option>
                    <option value="4">Dinners</option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label id="numeroTarjeta" for="numeroTarjeta" class="control-label col-md-4">N° de tarjeta</label>
            <div class="col-md-10">
                <input type="number" data-mask="0000 0000 0000 0000" name="numeroTarjeta" id="numeroTarjeta" value="" class="form-control text-box single-line" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" />
            </div>
        </div>
        <div class="form-group">
            <label id="fechaCaducidad" for="fechaCaducidad" class="control-label col-md-4">Fecha de Caducidad</label>
            <div class="col-md-10">
                <input type="text" name="fechaCaducidad" id="fechaCaducidad" placeholder="MM/YYYY" pattern="(0[1-9]|1[012])[/]((20)[0-9]{2}|[0-9]{2})" maxlength="7" class="form-control text-box single-line" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" />
            </div>
        </div>
        <div class="form-group">
            <label id="ccv" for="ccv" class="control-label col-md-4">Código de seguridad CCV</label>
            <div class="col-md-10">
                <input type="password" name="ccv" id="ccv" value="" class="form-control text-box single-line" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" />
            </div>
        </div>
        <div class="form-group">
            <label id="nombreTarjeta" for="nombreTarjeta" class="control-label col-md-4">Nombre del titular</label>
            <div class="col-md-10">
                <input type="text" name="nombreTarjeta" id="nombreTarjeta" value="" class="form-control text-box single-line" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" />
            </div>
        </div>
        <div class="form-group">
            <label id="nickname" for="nickname" class="control-label col-md-4">Alias de la tarjeta</label>
            <div class="col-md-10">
                <input type="text" name="nickname" id="nickname" value="" class="form-control text-box single-line" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" />
            </div>
        </div>
        <div class="form-group">
            <label id="favorita" for="favorita" class="control-label col-md-4">Tarjeta favorita</label>
            <div class="col-md-10">
                <input class="class-forms" type="checkbox" id="favorita" name="favorita" value="true" checked=""><br>
            </div>
        </div>
        <br>
        <div class="form-group">
            <div class="col-md-offset-6 col-md-10">
                <input type="submit" value="Enviar" class="btn btn-primary" />
            </div>
        </div>
    </div>

}
