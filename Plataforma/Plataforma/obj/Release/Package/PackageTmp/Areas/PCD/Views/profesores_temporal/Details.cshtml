@model Plataforma.Areas.PCD.Models.profesores_temporal

@{
    ViewBag.Title = "Detalles";
}
<link rel="stylesheet" type="text/css" href="~/Content/custombootstrap.css" />
<h2>Detalles</h2>

<div class="col-lg-9">
    <h4>Profesor</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
            @Html.DisplayFor(model => model.apellidos)
        </dd>

        <dt>
            Colegio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.colegio)
        </dd>

        <dt>
            Usuario
        </dt>

        <dd>
            @Html.DisplayFor(model => model.username)
        </dd>

        <dt>
            Correo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.correo)
        </dd>

        <dt>
            Telefono
        </dt>

        <dd>
            @Html.DisplayFor(model => model.telefono)
        </dd>

        <dt>
            Información Adicional
        </dt>

        <dd>
            @Html.DisplayFor(model => model.informacion_opcional)
        </dd>

    </dl>

    <div class="col-lg-9">
        <p>
            @Html.ActionLink("Editar", "Edit", new { id = Model.id }) |
            @Html.ActionLink("Volver al indice", "Index")
        </p>
    </div>
</div>