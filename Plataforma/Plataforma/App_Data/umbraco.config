    <libros id="2305" key="4c5f514a-1358-46cc-8009-d0b93c2fa656" parentID="1151" level="2" creatorID="3" sortOrder="31" createDate="2017-06-12T20:23:44" updateDate="2017-06-16T17:31:46" nodeName="Ediciones Personalizadas" urlName="ediciones-personalizadas" path="-1,1151,2305" isDoc="" nodeType="2205" creatorName="Allan Moya" writerName="Cesar Mauricio Obando Solano" writerID="0" template="2206" nodeTypeAlias="libros">
