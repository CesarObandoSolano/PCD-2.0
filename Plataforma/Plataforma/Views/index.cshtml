                  <div class="col-md-8 col-md-offset-1"> </div>
                </div>
              </div>
            </div>
          </div>
          <div class="mbr-section mbr-section-hero carousel-item dark center" data-bg-video-slide="false">
            <div class="mbr-table-cell">
              <div class="mbr-overlay"></div>
              <div class="container-slide"> <img src="@Umbraco.Media(CurrentPage.banner3).url">
                <div class="row">
                  <div class="col-md-8 col-md-offset-3 text-xs-right"> </div>
                </div>
              </div>
            </div>
          </div>
          <div class="mbr-section mbr-section-hero carousel-item dark center" data-bg-video-slide="false">
            <div class="mbr-table-cell">
              <div class="mbr-overlay" style="opacity: 0.3;"></div>
              <div class="container-slide"> <img src="@Umbraco.Media(CurrentPage.banner4).url">
